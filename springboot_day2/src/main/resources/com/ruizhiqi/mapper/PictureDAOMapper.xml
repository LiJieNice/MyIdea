<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" " http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.ruizhiqi.dao.PictureDAO">


    <update id="upload" parameterType="Picture">
        update t_titlecontent_copy set imgContent=#{imgContent},imgPath=#{imgPath},oldName=#{oldName},newName=#{oldName}
        where id=#{id}
    </update>


</mapper>
